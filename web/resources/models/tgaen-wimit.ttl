    skos:prefLabel "Poetic Voice"@en ;
    rdfs:label "Poetic Voice"@en ;
    rdfs:comment "Gray's engagement with issues of poetic voice, such as his engagement with the media of voice and print."@en ;
    crm:P3_has_note "Gray's engagement with issues of poetic voice, such as his engagement with the media of voice and print."@en ;
    crm:P67_refers_to dbpedia:Poetry ;
