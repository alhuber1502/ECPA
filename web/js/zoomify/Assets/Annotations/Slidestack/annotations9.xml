<ZAS>
	<SETUP 
		PANELPOSITION="2"
		INITIALVISIBILITY="1" 
		MINSCALE="0.20" 
		MAXSCALE="2" 
		POIVISIBILITY="1" 
		LABELVISIBILITY="1" 
		NOTEVISIBILITY="1" >
	</SETUP>
	<POI 
		ID="0" 
		NAME="Whole Image-SLIDE9" 
		X="center" 
		Y="center" 
		ZOOM="-1" 
		USER="Example User 1" 
		DATE="19990901200444" >
		<NOTES>
			<NOTE 
				ID="0" 				
				NAME="First example note" 
				TEXT="9 - Notes may be added here. Be sure to check out the editable demos of this annotation system (thumbnails below)." 
				USER="Example User 1" 
				DATE="20090101073026" >
			</NOTE>
			<NOTE 
				ID="1" 
				NAME="Many notes possible" 
				TEXT="This point of interest (POI) has multiple notes and multiple notes." 
				USER="Example User 1" 
				DATE="20090102073026" >
			</NOTE>
			<NOTE 
				ID="2" 
				NAME="Very long notes possible" 
				TEXT="This note is somewhat long. Notes can hold a virtually unlimited number of characters (spaces included)." 
				USER="Example User 1" 
				DATE="20090103110557" >
			</NOTE>
			<NOTE 
				ID="3" 
				NAME="Automatic scrollbar" 
				TEXT="A vertical scrollbar automatically appears when a note requires more lines than fit in the note area - as is the case with this note which is not verbose and yet is longer than will fit in the text area without a need to scroll to display the latter part of the note."
				USER="Example User 1" 
				DATE="20090104110557" >
			</NOTE>
			<NOTE 
				ID="4" 
				NAME="Carriage returns supported" 
				TEXT="Notes can contain carriage returns - new lines created with the Return key - like this... %0D or this %0D or this %0D ...very useful."
				USER="Example User 1" 
				DATE="20090105110557" >
			</NOTE>
		</NOTES>	
		<LABELS>
			<LABEL 
